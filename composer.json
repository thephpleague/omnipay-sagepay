{
    "name": "omnipay/sagepay",
    "type": "library",
    "description": "Sage Pay driver for the Omnipay PHP payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase",
        "sage pay",
        "sagepay"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-sagepay",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-sagepay/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\SagePay\\" : "src/"
        }
    },
    "require": {
        "omnipay/common": "~3.0"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^2.8.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
